{% extends "base.html.twig" %}
{% block body %}
	<div class="container">
		<div class="row">

    		
      		{% for product in products %}
      			<div class="col-sm">
      				
					<h6>{{product.name}}</h6>
					
					<a href="/product/{{product.id}}">
					<img src="{{ asset('images/' ~ product.image ~ '') }}" alt="{{product.name}} "class="shoes"/>
					</a>
					</div> 

			
			{% endfor %}
      
		</div>
	</div>








{% endblock %}